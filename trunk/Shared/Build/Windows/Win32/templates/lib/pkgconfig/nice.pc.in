libdir=@SHARED_BUILD_DIR@/lib
includedir=@SHARED_BUILD_DIR@/include

Name: libnice
Description: ICE library
Requires: glib-2.0 >= 2.10.0
Version: 0.0.13
Libs: -L${libdir} -lnice -Le:/Development/OSSBuild/Build/Windows/Win32/Release/lib -lgobject-2.0 -lgthread-2.0 -lglib-2.0 -lintl  

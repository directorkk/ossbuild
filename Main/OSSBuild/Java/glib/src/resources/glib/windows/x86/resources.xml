<?xml version="1.0" encoding="UTF-8"?>

<Resources name="glib">

	<References>
		<Add name="base" />
	</References>
	
	<Extract package="resources.glib.windows.x86.bin" directory="${tmp}/ossbuild/bin/">
		<Echo key="res.glib.init" msg="Initializing GLib..." />

		<!-- Libraries -->

		<Library resource="libglib-2.0-0.dll" />
		<Library resource="libgobject-2.0-0.dll" />
		<Library resource="libgmodule-2.0-0.dll" />
		<Library resource="libgthread-2.0-0.dll" />
		<Library resource="libgio-2.0-0.dll" />
	</Extract>

</Resources>